{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Zone</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ entity.libelle }}</td>
            </tr>
            <tr>
                <th>Education</th>
                <td>{{ entity.education }}</td>
            </tr>
            <tr>
                <th>Environement</th>
                <td>{{ entity.environement }}</td>
            </tr>
            <tr>
                <th>Secoursetinterventionurgence</th>
                <td>{{ entity.secoursetinterventionurgence }}</td>
            </tr>
            <tr>
                <th>Sante</th>
                <td>{{ entity.sante }}</td>
            </tr>
            <tr>
                <th>Loisirs</th>
                <td>{{ entity.loisirs }}</td>
            </tr>
            <tr>
                <th>Securite</th>
                <td>{{ entity.securite }}</td>
            </tr>
            <tr>
                <th>Transport</th>
                <td>{{ entity.transport }}</td>
            </tr>
            <tr>
                <th>Eauxusees</th>
                <td>{{ entity.eauxusees }}</td>
            </tr>
            <tr>
                <th>Nbrvote</th>
                <td>{{ entity.nbrvote }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('zone') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('zone_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
